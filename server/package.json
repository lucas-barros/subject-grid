{
  "name": "subject-grid-server",
  "version": "1.0.0",
  "description": "Subject grid server",
  "main": "index.js",
  "author": "Lucas Araujo",
  "scripts": {
    "start": "node --loader ts-node/esm index.ts",
    "build": "tsc",
    "dev": "tsnd --respawn --files index.ts",
    "start:development": "npm run dev",
    "test:unit": "jest --testMatch='<rootDir>/src/**/*.unit.test.ts'",
    "test:e2e": "jest --testMatch='<rootDir>/src/**/*.e2e.test.ts'",
    "typeorm": "typeorm-ts-node-esm",
    "lint": "eslint .",
    "format": "prettier --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "prettier": "3.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "joi": "^17.13.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "ts-results-es": "^4.1.0-alpha.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1",
    "zod": "^3.23.8"
  }
}
